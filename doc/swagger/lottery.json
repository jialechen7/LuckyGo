      "title": "Lottery",
      "required": [
        "id",
        "userId",
        "name",
        "thumb",
        "publishTime",
        "joinNumber",
        "introduce",
        "awardDeadline",
        "isSelected",
        "announceType",
        "announceTime",
        "isAnnounced",
        "sponsorId"
      ]
