        "thumb": {
          "type": "string",
          "description": "默认一等奖配图"
        },
        "publishTime": {
          "type": "integer",
          "format": "int64",
          "description": "发布抽奖时间"
        },
        "joinNumber": {
          "type": "integer",
          "format": "int64",
          "description": "自动开奖人数标准"
        },
        "introduce": {
          "type": "string",
          "description": "抽奖说明"
        },
        "awardDeadline": {
          "type": "integer",
          "format": "int64",
