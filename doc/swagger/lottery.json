        },
        "isSelected": {
          "type": "integer",
          "format": "int64",
          "description": "是否精选 1是 0否"
        },
        "announceType": {
          "type": "integer",
          "format": "int64",
          "description": "开奖设置：1按时间开奖 2按人数开奖 3即抽即中"
        },
        "announceTime": {
          "type": "integer",
          "format": "int64",
          "description": "开奖时间"
        },
        "isAnnounced": {
          "type": "integer",
          "format": "int64",
          "description": " 是否已经开奖：0未开奖 1已开奖"
        },
        "sponsorId": {
          "type": "integer",
          "format": "int64",
          "description": " 赞助商Id"
        }
